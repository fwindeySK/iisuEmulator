      <Content><![CDATA[--Script used for CI: Hand Position, Hand Orientation Hand Open/Close State--


function init()

--CI Origin (center of space)
spaceOrigin = {0,0.5,0};
handPosition = SK.Vector3();
hand2DPos = SK.Vector2();
horizontalDist = HorizontalCoeficient;
verticalDist = VerticalCoeficient;
depthLimitStart = DepthStart;
depthLimitEnd = DepthEnd;
handActive = false;
handClose = false;
Smooth_handHorizontal = SK.Average() -- Move this line to init()
Smooth_handVertical = SK.Average() -- Move this line to init()
Smooth_handDepth = SK.Average() -- Move this line to init()
smoothAmount = Smoothing;
throw = false;
startStart = false;

	horizontalPointingLeftBorder = 30
	horizontalPointingRightBorder = 30
	verticalPointingTopBorder = 30
	verticalPointingBottomBorder = 30
	
end


--clamp function
function clamp(value,min,max)
if value>max
then value = max
end
if value<min
then value = min
end
return value
end


--normalize function
function normalize(inValue, lowRange, highRange)
return ((inValue - lowRange)/(highRange - lowRange));
end

function getHandHorizontalNormalized(ciPalm2D, cameraDepthWidth)

	horizontal2D = SK.Math_clamp(ciPalm2D.x, horizontalPointingLeftBorder, cameraDepthWidth - horizontalPointingRightBorder)

	return (horizontal2D - horizontalPointingLeftBorder)/(cameraDepthWidth - (horizontalPointingLeftBorder + horizontalPointingRightBorder))

end

function getHandVerticalNormalized(ciPalm2D, cameraDepthHeight)

	vertical2D = SK.Math_clamp(ciPalm2D.y, verticalPointingTopBorder, cameraDepthHeight - verticalPointingBottomBorder)

	return (vertical2D - verticalPointingTopBorder)/(cameraDepthHeight - (verticalPointingBottomBorder + verticalPointingTopBorder))

end

function main()

throw = false;

--Position of palm/hand
ciHandPos = CI_HAND1_PalmPosition3D;

--Orientation of palm/hand
palmOrient = CI_HAND1_PalmNormal3D;

--the endpoint of the hand, located at some average point at end of fingers tip
fingerTip = CI_HAND1_TipPosition3D;

--vector point from center of hand to fingerTip
topVector = fingerTip - CI_HAND1_PalmPosition3D;


--cross product to obtain local right pointing vector for palm 
--and up pointing vector for palm
rightAxis = palmOrient:cross(SK.Axis_UP);
upAxis = rightAxis:cross(palmOrient);


--get the rightAxis vector comging from palmcenter
localRightAxis = ciHandPos + rightAxis;




---------------YAW--------------------
--get the yaw angle
yawAngle = SK.axisAngle(palmOrient, SK.Axis_FRONT, SK.Axis_RIGHT)+90;
yawValue = yawAngle/180;
yawValue = clamp(yawValue,0,1);
--------------------------------------
 


-------------PITCH--------------------
--get the pitchAngle angle
pitchAngle = SK.axisAngle(palmOrient, SK.Axis_FRONT, SK.Axis_UP)+90;
pitchValue = pitchAngle/180;
pitchValue = clamp(pitchValue,0,1);
--------------------------------------


-------------ROLL--------------------
--get the roll angle
rollAngle = SK.axisAngle(topVector,upAxis, localRightAxis)+90;
rollAngle = math.max(0, math.min(180,rollAngle));
rollValue = rollAngle/180;
rollValue = clamp(rollValue,0,1);
--------------------------------------



------------POSITION------------------

handPosition.x = -ciHandPos.x;
handPosition.y = ciHandPos.y;
handPosition.z = -ciHandPos.z;

--Amplifier based on hand distance to cam
positionAmplifier = math.sqrt(-math.pow(handPosition.y,2)+ 0.8429);



------Hand Horizontal Position-------------
handPosition.x = handPosition.x * positionAmplifier;
handPosition.x = clamp(handPosition.x, -horizontalDist, horizontalDist)

--normalize
handHorizontal = normalize(handPosition.x, -horizontalDist, horizontalDist);
-------------------------------------------



------Hand Vertical Position--------------
handPosition.z = handPosition.z * positionAmplifier;
handPosition.z = clamp(handPosition.z, -verticalDist, verticalDist)

--normalize
handVertical = normalize(handPosition.z, -verticalDist, verticalDist);
-------------------------------------------




------Hand Depth Position------------------
handPosition.y = clamp(handPosition.y, depthLimitStart,depthLimitEnd);
handDepth = handPosition.y;

--normalize 
handDepth = normalize(handDepth, depthLimitStart, depthLimitEnd)

-------------------------------------------




------------HAND STATES---------------


---------HAND1 ACTIVE/INNACTIVE-------
if (CI_HAND1_Status >=1)
then handActive = true;
else handActive = false;
end
--------------------------------------


------------CLOSE/OPEN - THROW----------------

if (CI_HAND1_IsOpen)

then handClose =  false;

	if(throwStart)
	then 	
	throwStart = false;
	throw = true;
	end

else handClose = true;

	if(not throwStart)
	then throwStart = true;
	throw = false;
	end


end
--------------------------------------




---------HAND1 GESTURES ID-------------
handGesture = CI_HAND1_PosingGestureId;
if (handGesture == 6)
then key1 = false; key2 = false; key3 = false; rightMouse = false;

elseif (handGesture == 4)
then key1 = true; key2 = false; key3 = false; rightMouse = false;

elseif (handGesture == 15)
then key1 = false; key2 = true; key3 = false; rightMouse = true;

elseif (handGesture == 9)
then key1 = false; key2 = false; key3 = true; rightMouse = false;

else
key1 = false; key2 = false; key3 = false; rightMouse = false;
end
--------------------------------------


----------------SMOOTHED HAND POSITIONS-------------------------------
handHorizontalsmooth = 1 - getHandHorizontalNormalized(CI_HAND1_PalmPosition2D, SOURCE_CAMERA_DEPTH_Width)

handVerticalsmooth = getHandVerticalNormalized(CI_HAND1_PalmPosition2D, SOURCE_CAMERA_DEPTH_Height)

handDepthsmooth = Smooth_handDepth:update(handDepth, smoothAmount)
---------------------------------------------------------------------


--for cartesian 2D data chart visualization
hand2DPos.x = handHorizontal;
hand2DPos.y = handVertical;

--------------------------------------
end

]]></Content>
